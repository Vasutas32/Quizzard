@using System.Globalization

<text x="@xText" y="@yText"
      fill="black"
      font-size="0.92em"
      text-anchor="middle"
      alignment-baseline="middle">
    @labelText
</text>

@code {
    [Parameter]
    public double xText { get; set; }
    [Parameter]
    public double yText { get; set; }
    [Parameter]
    public string labelText { get; set; } = string.Empty;
}